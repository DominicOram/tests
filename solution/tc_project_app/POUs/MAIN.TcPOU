	aFBAxes: ARRAY [1..axisNum] OF FB_Axis;
	//M1:FB_DriveVirtual;
	//M2:FB_DriveVirtual;
	//M3:FB_DriveVirtual;
	//M4:FB_DriveVirtual;
	//M5:FB_DriveVirtual;
	//M6:FB_DriveVirtual;
	//M7:FB_DriveVirtual;
	//M8:FB_DriveVirtual;
	
